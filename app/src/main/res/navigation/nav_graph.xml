<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_graph"
    app:startDestination="@id/loginFragment">

    <fragment
        android:id="@+id/loginFragment"
        android:name="cl.openworld.platformdev.ptkotlin.presentation.login.LoginFragment"
        android:label="LoginFragment">
        <action
            android:id="@+id/action_loginFragment_to_homeMapsFragment"
            app:destination="@id/homeMapsFragment"
            app:enterAnim="@anim/fragment_open_enter" />
    </fragment>
    <fragment
        android:id="@+id/homeMapsFragment"
        android:name="cl.openworld.platformdev.ptkotlin.presentation.home.HomeMapsFragment"
        android:label="HomeMapsFragment">
        <argument
            android:name="profile"
            app:argType="cl.openworld.platformdev.ptkotlin.data.models.Profile" />
    </fragment>
</navigation>